{
  "name": "janoscholtmanns/contao-teaser-bundle",
  "type": "contao-bundle",
  "description": "Contao 4 bundle for simple teasers",
  "license": "LGPL-3.0+",
  "authors": [
    {
      "name": "Janosch Oltmanns"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.1",
    "contao/core-bundle": "^4.13"
  },
  "conflict": {
    "contao/core": "*",
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "autoload": {
    "psr-4": {
      "JanoschOltmanns\\ContaoTeaserBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/languages/",
      "src/Resources/contao/templates/"
    ]
  },
  "extra": {
    "contao-manager-plugin": "JanoschOltmanns\\ContaoTeaserBundle\\ContaoManager\\Plugin"
  }
}
